{
  "name": "turf-difference",
  "version": "2.0.1",
  "description": "[Turf](http://turfjs.org/) difference module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js test/*.js",
    "pretest": "npm run lint",
    "test": "tape test/*.js",
    "doc": "dox -r < index.js | doxme --readme > README.md"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": [
        1
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Turfjs/turf-difference.git"
  },
  "keywords": [
    "turf",
    "gis"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf-difference/issues"
  },
  "homepage": "https://github.com/Turfjs/turf-difference",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "dox": "^0.7.1",
    "doxme": "^1.8.2",
    "eslint": "^1.10.3",
    "eslint-config-mourner": "^1.0.1",
    "glob": "~5.0.5",
    "tape": "~4.0.0"
  },
  "dependencies": {
    "jsts": "~0.15.0",
    "turf-featurecollection": "^1.0.1"
  }
}
